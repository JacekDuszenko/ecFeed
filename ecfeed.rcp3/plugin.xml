<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.testify.ecfeed.rcp3.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Perspective"
            class="com.testify.ecfeed.rcp3.Perspective"
            id="ecfeed.rcp3.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               label="File">
            <command
                  commandId="ecfeed.rcp3.new"
                  label="New...">
            </command>
            <command
                  commandId="ecfeed.rcp3.open"
                  label="Open...">
            </command>
            <command
                  commandId="ecfeed.rcp3.save"
                  label="Save"
                  style="push">
            </command>
            <command
                  commandId="ecfeed.rcp3.saveAs"
                  label="SaveAs..."
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.file.exit"
                  label="Exit">
            </command>
         </menu>
         <menu
               label="Edit">
            <command
                  commandId="org.eclipse.ui.edit.undo"
                  label="Undo">
            </command>
            <command
                  commandId="org.eclipse.ui.edit.redo"
                  label="Redo"
                  style="push">
            </command>
         </menu>
         <menu
               label="Help">
            <command
                  commandId="ecfeed.rcp3.about"
                  label="About"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="ecfeed.rcp3.open"
            name="Open">
      </command>
      <command
            id="ecfeed.rcp3.save"
            name="Save">
      </command>
      <command
            id="ecfeed.rcp3.saveAs"
            name="SaveAs">
      </command>
      <command
            id="ecfeed.rcp3.new"
            name="New">
      </command>
      <command
            id="ecfeed.rcp3.about"
            name="About">
      </command>
      <command
            id="ecfeed.rcp3.cut"
            name="Cut">
      </command>
      <command
            id="ecfeed.rcp3.copy"
            name="Copy">
      </command>
      <command
            id="ecfeed.rcp3.paste"
            name="Paste">
      </command>
      <command
            id="ecfeed.rcp3.delete"
            name="Delete">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.testify.ecfeed.rcp3.handlers.OpenEctHandler"
            commandId="ecfeed.rcp3.open">
      </handler>
      <handler
            class="com.testify.ecfeed.rcp3.handlers.SaveEctHandler"
            commandId="ecfeed.rcp3.save">
      </handler>
      <handler
            class="com.testify.ecfeed.rcp3.handlers.SaveAsEctHandler"
            commandId="ecfeed.rcp3.saveAs">
      </handler>
      <handler
            class="com.testify.ecfeed.rcp3.handlers.NewEctHandler"
            commandId="ecfeed.rcp3.new">
      </handler>
      <handler
            class="com.testify.ecfeed.rcp3.handlers.AboutHandler"
            commandId="ecfeed.rcp3.about">
      </handler>
      <handler
            class="com.testify.ecfeed.rcp3.handlers.CutHandler"
            commandId="ecfeed.rcp3.cut">
      </handler>
      <handler
            class="com.testify.ecfeed.rcp3.handlers.CopyHandler"
            commandId="ecfeed.rcp3.copy">
      </handler>
      <handler
            class="com.testify.ecfeed.rcp3.handlers.PasteHandler"
            commandId="ecfeed.rcp3.paste">
      </handler>
      <handler
            class="com.testify.ecfeed.rcp3.handlers.DeleteHandler"
            commandId="ecfeed.rcp3.delete">
      </handler>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="ecfeed.rcp3.application"
            name="ecFeed">
         <property
               name="appName"
               value="ecFeed">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="ecfeed.rcp3.open"
            schemeId="ecFeedScheme"
            sequence="M1+O">
      </key>
      <scheme
            id="ecFeedScheme"
            name="ecFeedScheme">
      </scheme>
   </extension>

</plugin>
